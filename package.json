{
  "name": "pack-it-up-v2-server",
  "private": true,
  "version": "1.0.0",
  "main": "./src/server.js",
  "description": "PackItUp server",
  "author": "CheoR <cheo.roman+github@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react-native",
    "apollo-server-express",
    "packing boxes",
    "shipping",
    "logistics"
  ],
  "scripts": {
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon build/server.js\"",
    "dev2": "yarn run build && yarn run start",
    "dev3": "nodemon --ext ts --exec 'yarn build --incremental && yarn serve'",
    "build": "tsc --build tsconfig.build.json ",
    "clean": "tsc --build --clean",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start": "node ./build/server.js",
    "start:dev": "babel-node src/server.ts",
    "test": "jest --config jest.config.ts ./src/__test__",
    "watch:node": "nodemon",
    "test:watch": "jest --verbose --detectOpenHandles",
    "watch:server": "nodemon ./build/server.js",
    "watch:tsc": "tsc -w"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "apollo-datasource-mongodb": "^0.5.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tools": "8.3.14",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
