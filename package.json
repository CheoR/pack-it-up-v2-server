{
  "name": "pack-it-up-v2-server",
  "private": true,
  "version": "1.0.0",
  "main": "./src/server.js",
  "description": "Personal Portfolio redone with Gatsby",
  "author": "CheoR <cheo.roman+github@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react-native",
    "apollo-server-express",
    "packing boxes",
    "shipping",
    "logistics"
  ],
  "scripts": {
    "dev": "yarn run build && yarn run serve",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "serve": "node ./build/server.js",
    "start:dev": "babel-node src/server.ts",
    "watch:server": "nodemon ./build/server.js",
    "watch:tsc": "tsc -w",
    "test": "jest",
    "test:watch": "jest --verbose --detectOpenHandles"
  },
  "dependencies": {
    "apollo-datasource-mongodb": "^0.5.4",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-tools": "4.x",
    "mongoose": "^6.5.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/mongoose": "^5.11.97",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
